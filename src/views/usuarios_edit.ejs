<%- include("partials/_header") %>

    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5">
                <div class="card">
                    <div class="card-body">
                        <form action="/update/<%= data.id %> " method="post">
                            <div class="form-group">
                                <input type="text" name="nombre"  value="<%= data.nombre %>"="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="documento" value="<%= data.documento %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="telefono" value="<%= data.telefono %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="email" value="<%= data.email %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="contraseña" value="<%= data.contraseña %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="estado" value="<%= data.estado %>" class="form-control">
                            </div>
                            <button type="submit" class="btn btn-primary">Guardar Usuario</button>
                        </form>
                        </form>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <%- include("partials/_footer") %>